{{#template}}
{{#ie}}
<!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge" /><![endif]-->
{{/ie}}
{{#doctype}}
{{#xhtml1}}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
{{/xhtml1}}
{{#html5}}
<!DOCTYPE html>
<html>
{{/html5}}
{{/doctype}}
<head>
	<title>{{name}}</title>
	<meta http-equiv="content-type" content="text/html; CHARSET={{charset}}" />
	{{#doctype.xhtml1}}
	<meta http-equiv="content-script-type" content="text/javascript" />
	<meta http-equiv="content-style-type" content="text/css" />
	{{/doctype.xhtml1}}
	<meta http-equiv="content-language" content="en" />
	<meta name="author" content="" />
	<meta name="robots" content="index,follow" />
	<meta name="description" content="" />
	<link rel="stylesheet" type="text/css" href="styles/{{name}}.css" />
	{{#ie}}
	{{#lte6}}
	<!--[if lte IE 6]>
	<link rel="stylesheet" type="text/css" href="styles/floatz-{{floatz.version}}/floatz.liquid.ie-lte6.css" />
	<![endif]-->
	{{/lte6}}
	{{#7}}
	<!--[if IE 7]>
	<link rel="stylesheet" type="text/css" href="styles/floatz-{{floatz.version}}/floatz.liquid.ie-7.css" />
	<![endif]-->
	{{/7}}
	{{#8}}
	<!--[if IE 8]>
	<link rel="stylesheet" type="text/css" href="styles/floatz-{{floatz.version}}/floatz.liquid.ie-8.css" />
	<![endif]-->
	{{/8}}
	{{#9}}
	<!--[if IE 9]>
	<link rel="stylesheet" type="text/css" href="styles/floatz-{{floatz.version}}/floatz.liquid.ie-9.css" />
	<![endif]-->
	{{/9}}
	{{/ie}}
	{{#floatz}}
	{{#scripts.lab}}
	<script type="text/javascript" src="styles/floatz-{{floatz.version}}/scripts/LAB-{{version}}.min.js"></script>
	{{/scripts.lab}}
	<script type="text/javascript" src="scripts/{{name}}.js"></script>
	{{/floatz}}
	</head>
<body>
{{#layout}}
<div id="flz_page">
	{{#sections.header}}
	{{#markup.xhtml1}}
	<div class="header">
		Header
	</div>
	{{/markup.xhtml1}}
	{{#markup.html5}}
	<header>
		Header
	</header>
	{{/markup.html5}}
	{{/sections.header}}
	{{#sections.nav}}
	{{#markup.xhtml1}}
	<div class="nav">
		Horizontal menu
	</div>
	{{/markup.xhtml1}}
	{{#markup.html5}}
	<nav>
		Horizontal menu
	</nav>
	{{/markup.html5}}
	{{/sections.nav}}
	{{#sections.sidenav}}
	{{#markup.xhtml1}}
	<div class="sidenav">
		Vertical menu
	</div>
	{{/markup.xhtml1}}
	{{#markup.html5}}
	<nav class="sidenav">
		Vertical menu
	</nav>
	{{/markup.html5}}
	{{/sections.sidenav}}
	{{#sections.content}}
	{{#markup.xhtml1}}
	<div class="article">
		Content
	</div>
	{{/markup.xhtml1}}
	{{#markup.html5}}
	<article>
		Content
	</article>
	{{/markup.html5}}
	{{/sections.content}}
	{{#sections.sidebar}}
	{{#markup.xhtml1}}
	<div class="aside">
		Sidebar
	</div>
	{{/markup.xhtml1}}
	{{#markup.html5}}
	<aside>
		Sidebar
	</aside>
	{{/markup.html5}}
	{{/sections.sidebar}}
	{{#sections.footer}}
	{{#markup.xhtml1}}
	<div class="footer">
		Footer
	</div>
	{{/markup.xhtml1}}
	{{#markup.html5}}
	<footer>
		Footer
	</footer>
	{{/markup.html5}}
	{{/sections.footer}}
</div>
</body>
{{/layout}}
{{/template}}